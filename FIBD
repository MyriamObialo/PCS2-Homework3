def num_rabbits(n, m):
    num_list=[]
    num_list.append(0)
    num_list.append(1)
    for x in range(1, n+1, 1):
        if x< m:
            num_list.append(num_list[x] + num_list[x-1])
        if x == m:
            num_list.append(num_list[x] + num_list[x-1] - num_list[x-m+1])
        if x > m:
            num_list.append(num_list[x] + num_list[x-1] - num_list[x-m])
    return num_list[n]
