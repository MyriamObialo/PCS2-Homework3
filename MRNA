def mRNA():
    file = open("/Users/myriamobialo/Downloads/rosalind_mrna.txt", "r")
    s= file.readline()
    s= s.replace('\n', '')
    file.close()
    all_codons = {'F': 2,'L': 6,'S': 6,'Y': 2,'C': 2,'W': 1,'P': 4,'H': 2,'Q': 2,'R': 6,'I': 3,'M': 1,'T': 4,'N': 2,'K': 2,'V': 4,'A': 4,'D': 2,'E': 2,'G': 4}
    n= 1
    for x in s:
        n= (n* all_codons[x]) % 1000000
    result= (n * 3) % 1000000
    print(result)
    return
